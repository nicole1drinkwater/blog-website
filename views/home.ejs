<%- include("header") -%>

<h1>Home</h1>
<p><%= homePageContent %></p>

<% journalPosts.forEach(function(post) { %>

    <h2><%= post.title %></h2>
    <p>
        <%= post.entry.substring(0, 100) + "..."%>
        <a href="/posts/<%= post.title %>">Read More</a>

    </p>

<% }) %>

<%- include("footer") -%>
