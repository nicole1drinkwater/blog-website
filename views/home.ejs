<%- include("header") -%>

<h1>Home</h1>
<p><%= homePageContent %></p>

<% journalPosts.forEach(function(post) { %>

    <h2><%= post.title %></h2>
    <p class="truncate-multi">
        <%= post.entry %>
        <a href="/posts/<%= _.lowerCase(post.title) %>">Read More</a>
    </p>
<% }) %>

<%- include("footer") -%>
